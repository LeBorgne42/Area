        <div class="offset-1 col-10">
            <div class="unlogin-block text-more">
                <div class="my-3">
                    <h3 class="text-vert2">{{ 'thanks'|trans|raw }}</h3>
                    <h2 class="text-or pt-5">{{ 'goalofthecrypto.title'|trans|raw }}</h2>
                    {{ 'goalofthecrypto.desc'|trans|raw }}
                    <h2 class="text-or pt-5">{{ 'generation.title'|trans|raw }}</h2>
                    {{ 'generation.desc'|trans|raw }}
