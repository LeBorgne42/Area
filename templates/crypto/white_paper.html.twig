                    {{ 'transfertingame.desc'|trans|raw }}
                    <h2 class="text-or pt-5">{{ 'buy.title'|trans|raw }}</h2>
                    {{ 'buy.desc'|trans|raw }}
                    <h2 class="text-or pt-5">{{ 'sell.title'|trans|raw }}</h2>
                    {{ 'sell.desc'|trans|raw }}
                    <h2 class="text-bronze pt-5">{{ 'season.title'|trans|raw }}</h2>
                    {{ 'season.desc'|trans|raw }}
                    <br>
                    {{ 'season.estimate'|trans|raw }}
