                        <a class="btn btn-lg btn-info mt-5" href="{{ path('building_add_brouilleur', {'idp': usePlanet.getId()}) }}">{{ 'building.build'|trans }}</a>
                    {% else %}
                        <a class="btn btn-lg btn-info mt-5" disabled>{{ 'building.build'|trans }}</a>
                    {% endif %}
                </td>
                <td class="center-cells p-4">
                    {% if usePlanet.getBuilding().getSkyBrouilleur().getLevel() > 0 %}
                        <a class="btn btn-lg btn-danger mt-5" href="{{ path('building_remove_brouilleur', {'idp': usePlanet.getId()}) }}">{{ 'building.demolish'|trans }}</a>
                    {% else %}
                        <a class="btn btn-lg btn-danger mt-5" disabled>{{ 'building.demolish'|trans }}</a>
                    {% endif %}
                </td>
            </tr>
            </tbody>
        </table>
    </div>
{% endblock %}
