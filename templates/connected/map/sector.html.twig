{% extends 'base.html.twig' %}

{% trans_default_domain 'front_map' %}

{% block title %}{{ 'title'|trans }}{% endblock %}

{% block body %}
    {% for message in app.flashes('fail') %}
        <div class="flash-notice mt-5 alerte-flash-error text-center">
            {{ message }}
        </div>
    {% endfor %}

    {% set topSector = sector.getId() - 10 %}
    {% if sector.getPosition() - 10 < 0 %}
        {% set topSector = sector.getId() + 90 %}
    {% endif %}
    {% set rightSector = sector.getId() + 1 %}
    {% if sector.getPosition() + 1 == 101 %}
        {% set rightSector = 1 %}
    {% endif %}
    {% set bottomSector = sector.getId() + 10 %}
    {% if sector.getPosition() + 10 > 100 %}
        {% set bottomSector = sector.getId() - 90 %}
    {% endif %}
    {% set leftSector = sector.getId() - 1 %}
    {% if sector.getPosition() - 1 == 0 %}
        {% set leftSector = 100 %}
    {% endif %}
    <div class="front-block galaxy">
        {% if app.user.getTutorial() == 15 or app.user.getTutorial() == 16 %}
        <table class="table table-striped borderOverview tutorial">
            <tbody>
            <tr>
                <th class="tab-cells-name p-3 tutorial">
                    <div class="row">
                        <div class="col-12 py-1">
                            {% if app.user.getTutorial() == 15  %}{{ 'sector.tutorial'|trans|raw }}{% elseif app.user.getTutorial() == 16  %}{{ 'sector.tutorialBis'|trans|raw }}{% endif %}
                        </div>
                    </div>
                </th>
            </tr>
            </tbody>
        </table>
        {% endif %}
        <div class="map-block-sector">
            {{ form_start(form_navigate) }}
            <div class="row mb-2 navigate">
                <div class="col-2 pt-1 text-more text-right">{{ form_label(form_navigate.galaxy) }}</div>
                <div class="col-1">
                    {{ form_widget(form_navigate.galaxy) }}
                </div>
                <div class="col-2 pt-1 text-more text-right">{{ form_label(form_navigate.sector) }}</div>
                <div class="col-1">
                    {{ form_widget(form_navigate.sector) }}
                </div>
                <div class="col-1">
                    {{ form_widget(form_navigate.sendForm) }}
                </div>
            </div>
            {{ form_end(form_navigate) }}
            <table class="table sector-table text-center sector">
                {% if planets|length > 1 %}
                <tbody>
                <tr>
                    <td class="special-cell"></td>
                    <td class="special-cell"></td>
                    <td class="special-cell"></td>
                    <td class="special-cell">
                        <span><a href="{{ path('map', {'sector': topSector, 'galaxy': galaxy.getId(), 'usePlanet': usePlanet.getId()}) }}"><span class="glyphicon glyphicon-chevron-up"></span></a></span>
                    </td>
                </tr>
                {% set x = 1 %}
                {% for planet in planets %}
                {% set fleetPresent = 0 %}
                {% if viewFleets %}
                    {% for viewFleet in viewFleets %}
                        {% if viewFleet.id == planet.id %}
                            {% set fleetPresent = 1 %}
                        {% endif %}
                    {% endfor %}
                {% endif %}
                {% if x == 1 %}
                <tr>
                    {% if planet.position != 11 %}
                        <td class="special-cell"></td>
                    {% endif %}
                    {% endif %}
                    {% if planet.position == 11 %}
                        <td class="special-cell">
                            <span><a href="{{ path('map', {'sector': leftSector, 'galaxy': galaxy.getId(), 'usePlanet': usePlanet.getId()}) }}"><span class="glyphicon glyphicon-chevron-left"></span></a></span>
                        </td>
                    {% endif %}
                    {% if planet.empty == false %}
                    {% if (viewSector and (viewSector.radar + viewSector.skyRadar) > planet.skyBrouilleur) or planet.character == character.getId() or (ally and planet.alliance == ally.getId()) or fleetPresent == 1 %}
                    <td class="center-cells pt-4">
                        {% if planet.nbCdr or planet.wtCdr %}
                            <div class="float-left"><span class="planet" data-html="true" data-toggle="tooltip" data-placement="top" title="{{ 'tooltip.cdr'|trans({'%niobium%': planet.nbCdr|number_format, '%water%': planet.wtCdr|number_format}) }}"><img alt="{{ 'image'|trans({},'front_global') }}" src="{{ asset('img/niobium.png') }}"></span></div>
                        {% endif %}
                        <div class="float-right">
                            {% if planet.cdr == false %}
                                <div><span data-html="true" data-toggle="tooltip" data-placement="top" title="{{ 'tooltip.planetInfos'|trans({'%land%': planet.ground, '%radar%': planet.radar + planet.skyRadar, '%brouilleur%': planet.skyBrouilleur, '%sky%': planet.sky}) }}">{% if planet.skyBrouilleur %}<img alt="{{ 'image'|trans({},'front_global') }}" src="{{ asset('img/radarjammed-known.gif') }}">{% elseif planet.radar or planet.skyRadar %}<img alt="{{ 'image'|trans({},'front_global') }}" src="{{ asset('img/radar.gif') }}">{% else %}<img alt="{{ 'image'|trans({},'front_global') }}" src="{{ asset('img/info.gif') }}">{% endif %}</span></div>
                            {% endif %}
                            {% if planet.fleets %}
                                {% set colorFleet = 'none' %}
                                <div>
                                    <a class="planet tipFleet" data-placement="right" data-trigger="" title="<h4 class='text-blanc'>{{ 'sector.fleet'|trans }}</h4>
                                                    {% for fleet in fleets %}
                                                    {% if fleet.planet == planet.id %}
                                                        {% set colorMyFleet = character.getFleetsColor(fleet.character, fleet.alliance) %}
                                                        {% if colorFleet != 'pp-enemy' %}
                                                            {% set colorFleet = character.getFleetsColor(fleet.character, fleet.alliance) %}
                                                        {% endif %}
                                                        <div class='my-1 text-left'><a class='{{ colorMyFleet }}' href='{% if fleet.character == character.getId() %}{{ path('manage_fleet', {'fleetGive': fleet.id, 'usePlanet': usePlanet.getId()}) }}{% elseif fleet.alliance %}{{ path('ally_profil', {'allyCharacter': fleet.allianceId, 'usePlanet': usePlanet.getId()}) }}{% else %}{{ path('user_profil', {'characterProfil': fleet.character, 'usePlanet': usePlanet.getId()}) }}{% endif %}'>{% if fleet.alliance %}[{{ fleet.alliance }}] {% endif %}{{ fleet.name }}</span></a> ({{ fleet.signature|number_format }})</span>{% if fleet.fightAt %}<span class='ml-2 text-rouge'>{{ 'fleet.combat'|trans }}</span>{% endif %}</div>
                                                    {% endif %}{% endfor %}{% if planet.signature > 0 %}{% endif %}"data-html="true">{% if colorFleet == 'pp-enemy' %}<img alt="{{ 'image'|trans({},'front_global') }}" src="{{ asset('img/fleets-enemy.gif') }}">{% elseif colorFleet == 'pp-mine' %}<img alt="{{ 'image'|trans({},'front_global') }}" src="{{ asset('img/fleets.gif') }}">{% elseif colorFleet == 'pp-ally' %}<img alt="{{ 'image'|trans({},'front_global') }}" src="{{ asset('img/fleets.gif') }}">{% elseif colorFleet == 'pp-peace' %}<img alt="{{ 'image'|trans({},'front_global') }}" src="{{ asset('img/fleets.gif') }}">{% endif %}
                                    </a>
                                </div>
                            {% endif %}
                            {% if planet.signature > 0 %}
                                <div>
                                    <a class="planet tipFleet" data-placement="right" data-trigger="" title="<h4 class='text-blanc'>{{ 'sector.groundfleet'|trans }}</h4><div class='my-1 text-left'>({{ planet.signature|number_format }}) <img height='15' src='{{ asset('img/utility.png') }}'></div>" data-html="true"><img alt="{{ 'image'|trans({},'front_global') }}" src="{{ asset('img/fleets-ground.gif') }}"></a>
                                </div>
                            {% endif %}
                        </div>
                        <a {% if planet.character == character.getId() %}href="{{ path('building', {'usePlanet': planet.id}) }}"{% else %} data-toggle="modal" data-target="#planetModal" href="{{ path('planet_interact', {'planet': planet.id, 'usePlanet': usePlanet.getId()}) }}"{% endif %}><img alt="{{ 'image'|trans({},'front_global') }}" class="mr-4 mb-4" {% if planet.character and planet.alliance %}data-toggle="tooltip" data-placement="top" title="{{ 'tooltip.planetAlly'|trans({'%name%': planet.username, '%ally%': planet.sigle }) }}"{% elseif planet.character %}data-toggle="tooltip" data-placement="top" title="{{ 'tooltip.planet'|trans({'%name%': planet.username }) }}"{% endif %} height="64" width="64" src="{{ vich_uploader_asset(planet, 'imageFile', 'App\\Entity\\Planet') }}"></a>
                        <div class="text-left ml-4{% if planet.character == character.getId() %} pp-mine{% elseif (ally and planet.alliance == ally.getId()) or character.getOurAllyPact(planet.alliance) %} pp-ally{% elseif planet.character %} pp-enemy{% elseif planet.cdr %} pp-cdr{% elseif planet.merchant %} pp-ia{% else %} pp-col{% endif %}">{% if planet.character and planet.zombie == 1 %}<img alt="{{ 'image'|trans({},'front_global') }}" src="{{ asset('img/special.gif') }}">{% endif %} {{ planet.name }} <span class="text-blanc text-middle">({{ planet.position }})</span></div>
                    </td>
                    {% else %}
                    <td class="center-cells pt-4">
                        <div class="float-right">
                            {% if planet.skyBrouilleur and (viewSector and (viewSector.radar + viewSector.skyRadar) > 0) %}
                                <div><span data-toggle="tooltip" data-placement="top" title="{{ 'tooltip.brouillage'|trans() }}"><img alt="{{ 'image'|trans({},'front_global') }}" src="{{ asset('img/radarjammed.gif') }}"></span></div>
                            {% endif %}
                        </div>
                        <a{% if planet.character == character.getId() %} href="{{ path('building', {'usePlanet': planet.id}) }}"{% else %} data-toggle="modal" data-target="#planetModal" href="{{ path('planet_interact', {'planet': planet.id, 'usePlanet': usePlanet.getId()}) }}"{% endif %}><img alt="{{ 'image'|trans({},'front_global') }}" class="mr-4 mb-4" data-toggle="tooltip" data-placement="top" title="{{ 'tooltip.planet'|trans({'%name%': planet.name }) }}" height="64" width="64" src="{{ vich_uploader_asset(planet, 'imageFile', 'App\\Entity\\Planet') }}"></a>
                        <div class="text-left ml-4{% if planet.cdr %} pp-cdr{% elseif planet.merchant %} pp-ia{% endif %}{% if planet.character %}{% if planet.character == character.getId() %} pp-mine{% elseif (ally and planet.sigle == ally.getSigle()) and planet.alliance != null  or character.getOurAllyPact(planet.sigle) %} pp-ally{% else %} pp-enemy{% endif %}">{% if planet.character and planet.zombie == 1 %}<img alt="{{ 'image'|trans({},'front_global') }}" src="{{ asset('img/special.gif') }}">{% endif %} {{ planet.name }} <span class="text-blanc text-middle">({{ planet.position }})</span></div>{% else %}">{% if planet.character and planet.zombie == 1 %}<img alt="{{ 'image'|trans({},'front_global') }}" src="{{ asset('img/special.gif') }}">{% endif %} <span class="{% if planet.merchant > 0 %}pp-ia2{% else %}pp-col{% endif %}">{{ planet.name }}</span> <span class="text-blanc text-middle">({{ planet.position }})</span></div>{% endif %}
        </td>
        {% endif %}
        {% else %}
        {% if (viewSector and (viewSector.radar + viewSector.skyRadar) > 0) or fleetPresent == 1 %}
        <td class="center-cells">
            {% if planet.nbCdr or planet.wtCdr %}
                <div class="float-left"><span class="empty" data-html="true" data-toggle="tooltip" data-placement="top" title="{{ 'tooltip.cdr'|trans({'%niobium%': planet.nbCdr|number_format, '%water%': planet.wtCdr|number_format}) }}"><img alt="{{ 'image'|trans({},'front_global') }}" src="{{ asset('img/niobium.png') }}"></span></div>
            {% endif %}
            <div class="float-right">
                {% if planet.character %}
                    <div><span class="mt-3 mr-2" data-html="true" data-toggle="tooltip" data-placement="top" title="{{ 'tooltip.radarInfos'|trans({'%radar%': planet.radar + planet.skyRadar, '%brouilleur%': planet.skyBrouilleur, '%character%': planet.username}) }}">{% if planet.skyBrouilleur %}<img alt="{{ 'image'|trans({},'front_global') }}" src="{{ asset('img/radarjammed-known.gif') }}">{% elseif planet.radar or planet.skyRadar %}<img alt="{{ 'image'|trans({},'front_global') }}" src="{{ asset('img/radar.gif') }}">{% else %}<img alt="{{ 'image'|trans({},'front_global') }}" src="{{ asset('img/info.gif') }}">{% endif %}</span></div>
                {% endif %}
                {% if planet.fleets %}
                    {% set colorFleet = 'none' %}
                    <div>
                        <a class="planet tipFleet" data-placement="right" data-trigger="" title="<h4 class='text-blanc'>{{ 'sector.fleet'|trans }}</h4>
                                                    {% for fleet in fleets %}
                                                    {% if fleet.planet == planet.id %}
                                                        {% set colorMyFleet = character.getFleetsColor(fleet.character, fleet.alliance) %}
                                                        {% if colorFleet != 'pp-enemy' %}
                                                            {% set colorFleet = character.getFleetsColor(fleet.character, fleet.alliance) %}
                                                        {% endif %}
                                                        <div class='my-1 text-left'><a class='{{ colorMyFleet }}' href='{% if fleet.character == character.getId() %}{{ path('manage_fleet', {'fleetGive': fleet.id, 'usePlanet': usePlanet.getId()}) }}{% elseif fleet.alliance %}{{ path('ally_profil', {'allyCharacter': fleet.allianceId, 'usePlanet': usePlanet.getId()}) }}{% else %}{{ path('user_profil', {'characterProfil': fleet.character, 'usePlanet': usePlanet.getId()}) }}{% endif %}'>{% if fleet.alliance %}[{{ fleet.alliance }}] {% endif %}{{ fleet.name }}</span></a> ({{ fleet.signature|number_format }})</span>{% if fleet.fightAt %}<span class='ml-2 text-rouge'>{{ 'fleet.combat'|trans }}</span>{% endif %}</div>
                                                    {% endif %}{% endfor %}"data-html="true">{% if colorFleet == 'pp-enemy' %}<img alt="{{ 'image'|trans({},'front_global') }}" src="{{ asset('img/fleets-enemy.gif') }}">{% elseif colorFleet == 'pp-mine' %}<img alt="{{ 'image'|trans({},'front_global') }}" src="{{ asset('img/fleets.gif') }}">{% elseif colorFleet == 'pp-ally' %}<img alt="{{ 'image'|trans({},'front_global') }}" src="{{ asset('img/fleets.gif') }}">{% elseif colorFleet == 'pp-peace' %}<img alt="{{ 'image'|trans({},'front_global') }}" src="{{ asset('img/fleets.gif') }}">{% endif %}
                        </a>
                    </div>
                {% endif %}
                {% if planet.signature > 0 %}
                    <div>
                        <a class="planet tipFleet" data-placement="right" data-trigger="" title="<h4 class='text-blanc'>{{ 'sector.groundfleet'|trans }}</h4><div class='my-1 text-left'>({{ planet.signature|number_format }}) <img alt='{{ 'image'|trans({},'front_global') }}' height='15' src='{{ asset('img/utility.png') }}'></div>" data-html="true"><img alt="{{ 'image'|trans({},'front_global') }}" src="{{ asset('img/fleets-ground.gif') }}"></a>
                    </div>
                {% endif %}
            </div>
            <div class="planet-position-empty">({{ planet.position }})</div>
        </td>
        {% else %}
        <td class="center-cells">
            <div class="planet-position-empty">({{ planet.position }})</div>
        </td>
        {% endif %}
        {% endif %}
        {% if planet.position == 15 %}
        <td class="special-cell">
            <span><a href="{{ path('map', {'sector': rightSector, 'galaxy': galaxy.getId(), 'usePlanet': usePlanet.getId()}) }}"><span class="glyphicon glyphicon-chevron-right"></span></a></span>
        </td>
        {% else %}
        {% endif %}
        {% set x = x + 1 %}
        {% if x == 6 %}
        {% set x = 1 %}
        </tr>
        {% endif %}
        {% endfor %}
        <tr>
            <td class="special-cell"></td>
            <td class="special-cell"></td>
            <td class="special-cell"></td>
            <td class="special-cell">
                <span><a href="{{ path('map', {'sector': bottomSector, 'galaxy': galaxy.getId(), 'usePlanet': usePlanet.getId()}) }}"><span class="glyphicon glyphicon-chevron-down"></span></a></span>
            </td>
        </tr>
        </tbody>
        {% endif %}
        </table>
    </div>
    {% if (viewSector and (viewSector.radar + viewSector.skyRadar) > 0) %}
        <div class="mt-5 sector-info">
            <div class="block-title"><h2>{{ 'map.current'|trans }} <span class="glyphicon glyphicon-question-sign text-orange text-middle" data-toggle="tooltip" data-placement="top" title="{{ 'tooltip.fleetHere'|trans }}"></span></h2></div>
            <table class="table table-striped table-bordered table-dark">
                <tbody>
                <tr>
                    <th class="tab-cells-name text-center">
                        {{ 'map.fleetName'|trans }}
                    </th>
                    <th class="tab-cells-name text-center">
                        {{ 'map.name'|trans }}
                    </th>
                    <th class="tab-cells-name text-center">
                        {{ 'map.from'|trans }}
                    </th>
                    <th class="tab-cells-name text-center">
                        {{ 'map.goTo'|trans }}
                    </th>
                    <th class="tab-cells-name text-center">
                        {{ 'map.time'|trans }}
                    </th>
                </tr>
                {% for fleet in fleetCurrent %}
                    <tr>
                        <th class="center-cells text-center">
                            <a class="{% if fleet.character == character.getId() %}text-bleu{% elseif (ally and fleet.alliance == ally.getId()) or fleet.merchant == 1 %}text-vert{% else %}text-rouge{% endif %}" href="{{ path('user_profil', {'characterProfil': fleet.character, 'usePlanet': usePlanet.getId()}) }}">{% if fleet.alliance %}[{{ fleet.sigle }}] {% endif %}{{ fleet.username }}</a>
                        </th>
                        <td class="center-cells text-center">
                            {% if fleet.character == character.getId() %}<span><a href="{{ path('manage_fleet', {'fleetGive': fleet.id, 'usePlanet': usePlanet.getId()}) }}">{{  fleet.name }}</a></span>{% else %}<span class="{% if (ally and fleet.alliance == ally.getId()) or fleet.merchant == 1 %}text-vert{% else %}text-rouge{% endif %}">{{  fleet.name }}</span>{% endif %} ({{ fleet.signature|number_format }})
                        </td>
                        <td class="center-cells text-center">
                            <span class="{% if fleet.pcharacter == character.getId() %}text-bleu{% elseif (ally and fleet.palliance == ally.getId())%}text-vert{% elseif fleet.pcharacter %}text-rouge{% endif %}">{{ fleet.planetname }} ({{ fleet.galaxy }}.{{ fleet.sector }}.{{ fleet.position }})</span>
                        </td>
                        <td class="center-cells text-center">
                            <span class="{% if fleet.dcharacter == character.getId() %}text-bleu{% elseif (ally and fleet.dalliance == ally.getId()) %}text-vert{% elseif fleet.dcharacter %}text-rouge{% endif %}">{{ fleet.dname }} ({{ fleet.galaxy }}.{{ fleet.sector }}.{{ fleet.dposition }})</span>
                        </td>
                        <td class="center-cells text-center">
                            <span class="timerArea" hidden>{{ fleet.flightTime|date('Y/m/d H:i:s') }}</span>
                        </td>
                    </tr>
                {% endfor %}
                </tbody>
            </table>
        </div>
        <div class="mt-2 sector-info">
            <div class="block-title"><h2>{{ 'map.coming'|trans }} <span class="glyphicon glyphicon-question-sign text-orange text-middle" data-toggle="tooltip" data-placement="top" title="{{ 'tooltip.fleetEntrance'|trans }}"></span></h2></div>
            <table class="table table-striped table-bordered table-dark">
                <tbody>
                <tr>
                    <th class="tab-cells-name text-center">
                        {{ 'map.fleetName'|trans }}
                    </th>
                    <th class="tab-cells-name text-center">
                        {{ 'map.name'|trans }}
                    </th>
                    <th class="tab-cells-name text-center">
                        {{ 'map.from'|trans }}
                    </th>
                    <th class="tab-cells-name text-center">
                        {{ 'map.goTo'|trans }}
                    </th>
                    <th class="tab-cells-name text-center">
                        {{ 'map.time'|trans }}
                    </th>
                </tr>
                {% for fleet in fleetIn %}
                    <tr>
                        <th class="center-cells text-center">
                            <a class="{% if fleet.character == character.getId() %}text-bleu{% elseif (ally and fleet.alliance == ally.getId()) or fleet.merchant == 1 %}text-vert{% else %}text-rouge{% endif %}" href="{{ path('user_profil', {'characterProfil': fleet.character, 'usePlanet': usePlanet.getId()}) }}">{% if fleet.alliance %}[{{ fleet.sigle }}] {% endif %}{{ fleet.username }}</a>
                        </th>
                        <td class="center-cells text-center">
                            {% if fleet.character == character.getId() %}<span><a href="{{ path('manage_fleet', {'fleetGive': fleet.id, 'usePlanet': usePlanet.getId()}) }}">{{  fleet.name }}</a></span>{% else %}<span class="{% if (ally and fleet.alliance == ally.getId()) or fleet.merchant == 1 %}text-vert{% else %}text-rouge{% endif %}">{{  fleet.name }}</span>{% endif %} ({{ fleet.signature|number_format }})
                        </td>
                        <td class="center-cells text-center">
                            {% if render(controller('App\\Service\\PlanetService::userRadarAction', {'sector': fleet.sector, 'galaxy': gal})) > fleet.skyBrouilleur or fleet.character == character.getId() %}<span><a href="{{ path('map', {'sector': fleet.sector, 'galaxy': fleet.galaxy, 'usePlanet': usePlanet.getId()}) }}">{{ fleet.planetname }} ({{ fleet.galaxy }}.{{ fleet.sector }}.{{ fleet.position }})</a></span>{% else %}<span class="text-rouge">???</span>{% endif %}
                        </td>
                        <td class="tab-cells-name text-center">
                            <span class="{% if fleet.dcharacter == character.getId() %}text-bleu{% elseif (ally and fleet.dalliance == ally.getId()) %}text-vert{% elseif fleet.dcharacter %}text-rouge{% endif %}">{{ fleet.dname }} ({{ fleet.dgalaxy }}.{{ fleet.dsector }}.{{ fleet.dposition }})</span>
                        </td>
                        <td class="center-cells text-center">
                            <span class="timerArea" hidden>{{ fleet.flightTime|date('Y/m/d H:i:s') }}</span>
                        </td>
                    </tr>
                {% endfor %}
                </tbody>
            </table>
        </div>
        <div class="mt-2 sector-info">
            <div class="block-title"><h2>{{ 'map.out'|trans }} <span class="glyphicon glyphicon-question-sign text-orange text-middle" data-toggle="tooltip" data-placement="top" title="{{ 'tooltip.fleetQuit'|trans }}"></span></h2></div>
            <table class="table table-striped table-bordered table-dark">
                <tbody>
                <tr>
                    <th class="tab-cells-name text-center">
                        {{ 'map.fleetName'|trans }}
                    </th>
                    <th class="tab-cells-name text-center">
                        {{ 'map.name'|trans }}
                    </th>
                    <th class="tab-cells-name text-center">
                        {{ 'map.from'|trans }}
                    </th>
                    <th class="tab-cells-name text-center">
                        {{ 'map.goTo'|trans }}
                    </th>
                    <th class="tab-cells-name text-center">
                        {{ 'map.time'|trans }}
                    </th>
                </tr>
                {% for fleet in fleetOut %}
                    <tr>
                        <th class="center-cells text-center">
                            <a class="{% if fleet.character == character.getId() %}text-bleu{% elseif (ally and fleet.alliance == ally.getId()) or fleet.merchant == 1 %}text-vert{% else %}text-rouge{% endif %}" href="{{ path('user_profil', {'characterProfil': fleet.character, 'usePlanet': usePlanet.getId()}) }}">{% if fleet.alliance %}[{{ fleet.sigle }}] {% endif %}{{ fleet.username }}</a>
                        </th>
                        <td class="center-cells text-center">
                            {% if fleet.character == character.getId() %}<span><a href="{{ path('manage_fleet', {'fleetGive': fleet.id, 'usePlanet': usePlanet.getId()}) }}">{{  fleet.name }}</a></span>{% else %}<span class="{% if (ally and fleet.alliance == ally.getId()) or fleet.merchant == 1 %}text-vert{% else %}text-rouge{% endif %}">{{  fleet.name }}</span>{% endif %} ({{ fleet.signature|number_format }})
                        </td>
                        <td class="center-cells text-center">
                            <span class="{% if fleet.character == character.getId() %}text-bleu{% elseif (ally and fleet.alliance == ally.getId()) %}text-vert{% elseif fleet.dcharacter %}text-rouge{% endif %}">{{ fleet.name }} ({{ fleet.galaxy }}.{{ fleet.sector }}.{{ fleet.position }})</span>
                        </td>
                        <td class="tab-cells-name text-center">
                            {% if render(controller('App\\Service\\PlanetService::userRadarAction', {'sector': fleet.dsector, 'galaxy': gal})) > fleet.skyBrouilleur or fleet.dcharacter == character.getId() %}<span><a href="{{ path('map', {'sector': fleet.dsector, 'galaxy': fleet.dgalaxy, 'usePlanet': usePlanet.getId()}) }}">{{ fleet.dname }} ({{ fleet.dgalaxy }}.{{ fleet.dsector }}.{{ fleet.dposition }})</a></span>{% else %}<span class="text-rouge">???</span>{% endif %}
                        </td>
                        <td class="center-cells text-center">
                            <span class="timerArea" hidden>{{ fleet.flightTime|date('Y/m/d H:i:s') }}</span>
                        </td>
                    </tr>
                {% endfor %}
                </tbody>
